<h2 style="font-weight:200" class="ps-3 mt-4">Edit</h2>
<div class="col-12 col-xl-10 col-lg-9 col-md-11 ">
	<div class="row mx-2 mt-1">
		<div class="col-12">
			<label for="inputEmail4 ">Title</label>
			<input class="form-control mt-1" id="title" type="text" placeholder="" value="title">
		</div>
	</div>
	<div class="row mx-2 mt-2">
		<div class="col-12 my-3">
			<label for="inputEmail4">Description</label>
			<textarea id="CMStext" value="CMStext" placeholder=""></textarea>
		</div>
	</div>
	<div class="row mx-2 mt-1">
		<div class="col-12">
			<label for="inputEmail4 ">Slug</label>
			<input class="form-control mt-1" id="slug" type="text" placeholder="" value="slug">
		</div>
	</div>
	<div class="row mx-2 mt-1">
		<div class="col-12">
			<label for="inputEmail4 ">Status</label>
			<input class="form-control mt-1" id="status" type="text" placeholder="" value="status">
		</div>
	</div>
</div>


<div class="row mx-2 mt-2">
	<div class="col-12 d-flex justify-content-end">
		<div>
			<a asp-controller="Admin" asp-action="CMSPage" class="btn btn-outline-warning me-2" style="border-radius: 50px;">Cancel</a>
			<button type="submit" class="btn btn-outline-warning" style="border-radius: 50px;" onclick="EdiCMSData()">Save</button>



		</div>
	</div>
</div>

